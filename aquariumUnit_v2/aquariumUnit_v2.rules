
(rules PCB aquariumUnit_v2
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.2)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.9)
    )
  )
  (rule
    (width 800.0)
    (clear 600.2)
    (clear 400.0 (type smd_to_turn_gap))
    (clear 150.0 (type smd_smd))
  )
  (padstack "Via[0-1]_2000:800_um"
    (shape
      (circle F.Cu 2000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 2000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_2000:800_um" "Via[0-1]_2000:800_um" default
  )
  (via 
    "Via[0-1]_2000:800_um-kicad_default" "Via[0-1]_2000:800_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_2000:800_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_2000:800_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 800.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    +3V3 GND /RX /TX /RST /D15 "Net-(R3-Pad1)" /D0
    /D16 "Net-(U1-Pad9)" "Net-(U1-Pad10)" /D9 /D10 "Net-(U1-Pad13)" "Net-(U1-Pad14)" /ADC
    /D14 /D12 /D13 /D2 /D4 /D5 "Net-(D1-Pad2)" "Net-(D2-Pad2)"
    +12V "Net-(P7-Pad1)" "Net-(P7-Pad2)" "Net-(P7-Pad3)" "Net-(P7-Pad4)" "Net-(P8-Pad2)" "Net-(R7-Pad2)" "Net-(U3-Pad13)"
    "Net-(U3-Pad11)" "Net-(U3-Pad10)" "Net-(U3-Pad9)" "Net-(U3-Pad7)" "Net-(U3-Pad6)" "Net-(U3-Pad4)" "Net-(U3-Pad2)" "Net-(U3-Pad15)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 800.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)